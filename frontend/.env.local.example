# Frontend Environment Configuration
# Copy this file to .env.local and update with your values

# API Service URLs (Development)
NEXT_PUBLIC_AUTH_URL=http://localhost:8001
NEXT_PUBLIC_TEXT_URL=http://localhost:8002
NEXT_PUBLIC_VOICE_URL=http://localhost:8003
NEXT_PUBLIC_FACE_URL=http://localhost:8004
NEXT_PUBLIC_FUSION_URL=http://localhost:8005
NEXT_PUBLIC_MOOD_JOURNAL_URL=http://localhost:8008

# App Configuration
NEXT_PUBLIC_APP_NAME=Mental Health Assistant
NEXT_PUBLIC_APP_VERSION=2.0.0
NEXT_PUBLIC_APP_DESCRIPTION=AI-powered mental health support

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_ANALYSIS=true
NEXT_PUBLIC_ENABLE_FACE_ANALYSIS=true
NEXT_PUBLIC_ENABLE_MULTIMODAL_FUSION=true

# Analytics (Optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# Production URLs (uncomment for production)
# NEXT_PUBLIC_AUTH_URL=https://api.yourdomain.com/auth
# NEXT_PUBLIC_TEXT_URL=https://api.yourdomain.com/text
# NEXT_PUBLIC_VOICE_URL=https://api.yourdomain.com/voice
# NEXT_PUBLIC_FACE_URL=https://api.yourdomain.com/face
# NEXT_PUBLIC_FUSION_URL=https://api.yourdomain.com/fusion
# NEXT_PUBLIC_DOCTOR_URL=https://api.yourdomain.com/doctor
# NEXT_PUBLIC_NOTIFICATION_URL=https://api.yourdomain.com/notification
# NEXT_PUBLIC_MOOD_JOURNAL_URL=https://api.yourdomain.com/mood
# NEXT_PUBLIC_REPORT_URL=https://api.yourdomain.com/report
